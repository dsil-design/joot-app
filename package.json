{
  "name": "joot",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPatterns=__tests__ --testPathIgnorePatterns=\"integration|accessibility|performance|e2e\"",
    "test:integration": "jest --testPathPatterns=integration",
    "test:accessibility": "jest --testPathPatterns=accessibility",
    "test:performance": "jest --testPathPatterns=performance",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:accessibility && npm run test:performance",
    "test:docs": "jest --testPathPatterns=docs --coverage --collectCoverageFrom='src/app/docs/**/*.{ts,tsx}' --collectCoverageFrom='src/components/docs/**/*.{ts,tsx}'",
    "test:supabase": "node scripts/db/test-supabase-connection.js",
    "db:reset-transactions": "node scripts/db/reset-transaction-data.js",
    "test:ci": "npm run test:coverage && npm run test:e2e",
    "validate:env": "node scripts/env/validate-env.js",
    "backfill:rates": "tsx scripts/backfill-historical-rates.ts",
    "backfill:rates:check": "tsx scripts/backfill-historical-rates.ts --check",
    "backfill:rates:dry": "tsx scripts/backfill-historical-rates.ts --dry-run"
  },
  "dependencies": {
